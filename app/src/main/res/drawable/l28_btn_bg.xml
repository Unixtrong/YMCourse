<?xml version="1.0" encoding="utf-8"?>
<shape
    xmlns:android="http://schemas.android.com/apk/res/android">

    <solid android:color="#DDDD33"/>
    <corners android:radius="20dp"/>
    <stroke
        android:width="5dp"
        android:color="#5555CC"
        android:dashGap="5dp"
        android:dashWidth="10dp"/>
    <padding
        android:bottom="5dp"
        android:left="30dp"
        android:right="30dp"
        android:top="5dp"/>
    <gradient
        android:angle="45"
        android:centerColor="#339933"
        android:endColor="#333399"
        android:gradientRadius="15dp"
        android:startColor="#993333"
        android:type="sweep"/>
    <size
        android:width="50dp"
        android:height="50dp"/>
</shape>